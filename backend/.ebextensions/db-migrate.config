container_commands:
  01_makemigration:
    command: "source /var/app/venv/*/bin/activate && python3 manage.py makemigration"
    leader_only: true
  02_migrate:
    command: "source /var/app/venv/*/bin/activate && python3 manage.py migrate"
    leader_only: true
option_settings:
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: backend.settings